<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
     PUBLIC "-//mybatis.org//DTDMapper3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.example.demo.mapper.KeywordMapper">

        <select id="keywordListByCat_id" resultType="com.example.demo.vo.KeywordVO">
            SELECT * FROM keyword 
            where cat_id=#{cat_id}
        </select>

        <select id="keywordListBykey_id" resultType="com.example.demo.vo.KeywordVO">
            SELECT * FROM keyword 
            where key_id=#{key_id}
        </select>

    </mapper>